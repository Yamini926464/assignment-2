// find whether the given number is prime or not
#include<stdio.h>
int main()
{
   int n,i=2,count=0;
    scanf("%d",&n);
    while( i<n)
    {  
        if(n%i==0)
        {
            count=1;
            break;
        }
        i++;
    }
 
    if (count==0)
        printf("PRIME",n);
    else
        printf("NOT PRIME",n);
 
  
    
}
