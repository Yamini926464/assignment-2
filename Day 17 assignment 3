#include<stdio.h>
int main()
{
   int n,i=2,count=0;
    scanf("%d",&n);
    while( i<=n/2)
    {  
        if(n%i==0)
        {
            count=1;
            break;
        }
        i++;
    }
 
    if (count==0)
        printf("%d prime number",n);
    else
        printf("%d not prime number",n);
 
  
    
}
